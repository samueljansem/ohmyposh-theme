{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#ffca85",
          "template": "$ {{ .UserName }} ",
          "style": "plain",
          "type": "session"
        },
        {
          "background": "transparent",
          "foreground": "#a277ff",
          "template": "{{ .Path }} ",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "#61ffca",
          "template": "@ {{ .HEAD }} ",
          "style": "plain",
          "type": "git",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}

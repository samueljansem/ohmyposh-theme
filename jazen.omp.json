{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#a277ff",
					"foreground": "#ffffff",
					"powerline_symbol": "\ue0b0",
					"properties": {
						"style": "full",
						"template": " {{ .Path }} "
					},
					"style": "powerline",
					"type": "path"
				},
				{
					"background": "#61ffca",
					"foreground": "#110F18",
					"powerline_symbol": "\ue0b0",
					"properties": {
						"template": " {{ .HEAD }} "
					},
					"style": "powerline",
					"type": "git"
				},
				{
					"background": "#FF6471",
					"foreground": "#ffffff",
					"leading_diamond": "<transparent,background>\ue0b0</>",
					"properties": {
						"template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
					},
					"style": "diamond",
					"trailing_diamond": "\ue0b4",
					"type": "python"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 1
}